CREATE TABLE users(id INTEGER PRIMARY KEY AUTO_INCREMENT NOT NULL, user VARCHAR(24) UNIQUE NOT NULL, password VARCHAR(128) NOT NULL, permission_level TINYINT NOT NULL);
CREATE TABLE sessions(enc_key VARCHAR(32) UNIQUE NOT NULL, nonce VARCHAR(128) UNIQUE NOT NULL, user_id INT UNIQUE NOT NULL, creation_time BIGINT NOT NULL);

